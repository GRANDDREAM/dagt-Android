<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/app_base_bg"
    android:orientation="vertical">

    <com.grandream.dagt.ui.navigationLayout.NavigationLayout
        android:id="@+id/navigation"
        android:layout_width="match_parent"
        android:layout_height="@dimen/m130px_dp"
        android:background="@color/title_bg"
        android:paddingTop="@dimen/m28px_dp"
        app:na_title="@string/app_name" />

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/navigation"
        android:background="@color/app_base_bg">

        <com.grandream.dagt.ui.ProgressWebView.ProgressWebView
            android:id="@+id/common_webview"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layerType="software" />


    </RelativeLayout>


</RelativeLayout>